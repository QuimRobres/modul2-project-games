<div class="card">
  <img src='{{this.thumb_url}}' alt='game-image'>
  <div>
    <a href="#/{{this.id}}">
      <h5>{{this.name}}</h5>
    </a>
    <p>Players: {{this.min_players}} - {{this.max_players}}</p>
    <p>Playtime: {{this.min_playtime}} - {{this.max_players}} mins</p>
    <p>Rating: {{this.average_user_rating}}</p>
    <p>{{this.price}}</p>
  </div>
</div>